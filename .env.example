# Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=authToken
VITE_SESSION_TIMEOUT=3600000
VITE_ENCRYPTION_KEY=EQMS_SECRET_KEY_2026

# Feature Flags
VITE_ENABLE_AUDIT_TRAIL=true
VITE_ENABLE_E_SIGNATURE=true
VITE_ENABLE_NOTIFICATIONS=true

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,png,jpg,jpeg

# Application
VITE_APP_NAME=Quality Management System
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Google Gemini API (if using AI features)
GEMINI_API_KEY=your_api_key_here
